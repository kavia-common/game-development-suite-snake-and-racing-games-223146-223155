# Example environment variables for Ocean Games UI
# Note: For Create React App, only variables prefixed with REACT_APP_ are exposed to the frontend.

# Logical runtime environment (affects EnvBanner visibility)
REACT_APP_NODE_ENV=development

# Logging level shown in banner (informational only)
REACT_APP_LOG_LEVEL=info

# Enable experimental features in UI banner
REACT_APP_EXPERIMENTS_ENABLED=false

# Feature flags JSON (ensure this is valid JSON)
# Example enables both games and status bar:
REACT_APP_FEATURE_FLAGS='{"enableSnake":true,"enableRacing":true,"showStatusBar":true}'

# Optional container-level variables (unused by the UI but part of container env)
REACT_APP_API_BASE=
REACT_APP_BACKEND_URL=
REACT_APP_FRONTEND_URL=
REACT_APP_WS_URL=
REACT_APP_NEXT_TELEMETRY_DISABLED=true
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=false
REACT_APP_HEALTHCHECK_PATH=/healthz
